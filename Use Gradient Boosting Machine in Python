from sklearn.ensemble import GradientBoostingRegressor #(or Classifier)

x_train = train[feature_name], y_train = train[label_name]
x_test = train[feature_name]

model = GradientBoostingRegressor(n_estimators = 1000,
                                  learning_rate = 0.01,
                                  random_state = 5)


model.fit(x_train, y_train)
model.predict(x_test)
